<% provide('title', 'ユーザーリスト') %>
<ul class="users">
<% @users.each do |user| %>
  <li>
    <%= link_to (image_tag "prof.jpg"), user %>
    <div class="prof_box">
      <h2><%= link_to user.name, user %></h2>
      <p>
        投稿数: XXX ,　いいね数：XXX
      </p>
      <p>
        <%= user.description %> 
      </p>
    </div>
  </li>
<% end %>
</ul>
